{
  "name": "merels",
  "version": "0.9.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache build out",
    "build": "yarn clean && parcel build index.html -d out --no-source-maps --no-cache --experimental-scope-hoisting --detailed-report --public-url ./",
    "start": "parcel index.html -p 8080 --no-cache"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "terser": "^5.2.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@thi.ng/api": "latest",
    "@thi.ng/atom": "latest",
    "@thi.ng/hdom": "latest",
    "@thi.ng/paths": "latest",
    "@thi.ng/strings": "^1.10.0",
    "@thi.ng/transducers": "latest",
    "apl": "^0.1.15"
  }
}
